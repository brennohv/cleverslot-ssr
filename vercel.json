{
  "version": 2,
  "public": true,
  "name": "dynamic-shell-updated",
  "rewrites": [
    {
      "source": "/custom-api/:path*",
      "destination": "https://typical-cheese-f4c89ba13c.strapiapp.com/api/:path*"
    },
    {
      "source": "/custom-graphql",
      "destination": "https://typical-cheese-f4c89ba13c.strapiapp.com/graphql"
    },
    { "source": "/(.*)", "destination": "/api" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/(.*)\\.(js|css|woff2?|ttf|otf|eot|svg|png|jpg|jpeg|gif|webp|ico)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "dist/dynamic-shell-updated/**",
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
